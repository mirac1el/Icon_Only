{
    "manifest_version": 3,
    "name": "Icon Only",
    "version": "1.0",
    "description": " Bookmark bar icon only. ",
    "permissions": [
      "bookmarks",
      "notifications"
    ],
    "background": {
      "service_worker": "background.js"
    },
    "action": {
      "default_title": "Icon Only",
      "default_icon": {
        "16": "icons/icon16.png",
        "48": "icons/icon48.png",
        "128": "icons/icon128.png"
      }
    },
    "icons": {
      "16": "icons/icon16.png",
      "48": "icons/icon48.png",
      "128": "icons/icon128.png"
    }
  }
  